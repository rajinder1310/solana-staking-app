  "accounts": [
    {
      "name": "UserStakeInfo",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "amount",
            "type": "u64"
          },
          {
            "name": "depositTs",
            "type": "i64"
          }
        ]
      }
    },
    {
      "name": "GlobalConfig",
      "type": {
        "kind": "struct",
        "fields": [
          {
            "name": "admin",
            "type": "publicKey"
          },
          {
            "name": "withdrawFeeBps",
            "type": "u64"
          }
        ]
      }
    }
  ],
  "events": [
    {
      "name": "TokensStaked",
      "fields": [
        {
          "name": "staker",
          "type": "publicKey",
          "index": false
        },
        {
          "name": "amount",
          "type": "u64",
          "index": false
        },
        {
          "name": "totalStaked",
          "type": "u64",
          "index": false
        }
      ]
    },
    {
      "name": "TokensWithdrawn",
      "fields": [
        {
          "name": "staker",
          "type": "publicKey",
          "index": false
        },
        {
          "name": "amount",
          "type": "u64",
          "index": false
        },
        {
          "name": "fee",
          "type": "u64",
          "index": false
        },
        {
          "name": "totalStaked",
          "type": "u64",
          "index": false
        }
      ]
    },
    {
      "name": "FeeUpdated",
      "fields": [
        {
          "name": "oldFee",
          "type": "u64",
          "index": false
        },
        {
          "name": "newFee",
          "type": "u64",
          "index": false
        }
      ]
    }
  ],
  "errors": [
    {
      "code": 6000,
      "name": "Unauthorized",
      "msg": "You are not authorized to perform this action."
    },
    {
      "code": 6001,
      "name": "InvalidAmount",
      "msg": "Amount must be greater than zero."
    },
    {
      "code": 6002,
      "name": "InvalidWithdraw",
      "msg": "No tokens to withdraw."
    }
  ]
